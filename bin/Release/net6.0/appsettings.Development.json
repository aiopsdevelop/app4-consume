{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseTracingExporter": "otlp",
  "UseMetricsExporter": "otlp",
  "UseLogExporter": "otlp",
  "Otlp": {
    "ServiceName": "devops",
    "Endpoint": "http://hybridlog.io:4344"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "Version": "1.0.0"
}
